<h1>This is student table page</h1>

<table>
    <thead>
        <tr>
            <th> First name</th>
            <th> Last name</th>
            <th> address</th>
        </tr>
    </thead>

    <tbody>
        <% @student.each do |student| %>
            <tr>
                <td><%= student.first_name %></td>
                <td><%= student.last_name %></td>
                <td><%= student.address %></td>
            </tr>
        <%end%>
    </tbody>
</table>